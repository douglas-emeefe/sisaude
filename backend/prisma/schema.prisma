generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  nome      String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Paciente {
  id               Int      @id @default(autoincrement())
  nome             String
  nomeMae          String?
  nomeAcompanhante String?
  profissao        String?
  dataNascimento   DateTime?
  sexo             String?
  residente        String?
  raca             String?
  etnia            String?
  cpf              String?   
  sus              String?
  rua              String?
  numero           String?
  bairro           String?
  createdAt        DateTime @default(now())
}
